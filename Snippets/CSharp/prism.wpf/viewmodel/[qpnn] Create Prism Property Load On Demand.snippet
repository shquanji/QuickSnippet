          <Default>属性描述</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="csharp" Delimiter="$"><![CDATA[/// <summary>
/// $属性描述$ 属性字段
/// <para>关联属性: $Field$</para>
/// </summary>
private $type$ _$field$;
/// <summary>
/// $属性描述$
/// </summary>
public $type$ $Field$
{
	get 
	{ 
		if(_$field$==null) _$field$ = new $type$();
		return _$field$;
	}
	set { SetProperty(ref _title, value); }
}]]></Code>
